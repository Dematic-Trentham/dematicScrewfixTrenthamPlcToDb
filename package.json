{
  "name": "plc-to-db",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node index.js",
    "startDev": "node build/index.js",
    "startDevWatch": "nodemon build/index.js",
    "startDev2Watch": "nodemon src/index.ts",
    "startDevDockInspect": "nodemon build/index.js --inspect-brk=0.0.0.0",
    "buildOnce": "tsc",
    "build": "tsc --watch",
    "prisma-merge": "prisma format && prisma generate && echo 'complete!'",
    "prisma-gen": "prisma generate --schema=./node_modules/masterPrismaSchema/prisma/schema",
    "prisma-pull": "npx prisma db pull --schema=./node_modules/masterPrismaSchema/prisma/schema",
    "prisma-push": "npx prisma db push --schema=./node_modules/masterPrismaSchema/prisma/schema",
    "build-docker": "npm run prisma-merge && npm run buildOnce && docker build -t screwfix-trentham-plc-to-db ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "bluebird": "^3.7.2",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "node-cron": "^3.0.2",
    "node-snap7": "^1.0.6"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.21",
    "@types/node-cron": "^3.0.7",
    "@types/node-snap7": "^0.2.21",
    "masterPrismaSchema": "git+https://jdematic@github.com/jameswlyons/dematicDashboardScrewfixTrenthamMasterPrismaSchema.git",
    "prisma": "^5.19.0"
  }
}
